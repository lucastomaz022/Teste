<?xml version="1.0" encoding="UTF-8"?>


<Configuration>
	<Appenders>
		<Console name="console" target="SYSTEM_OUT">
			<PatternLayout 
			 pattern="[%-5level] %d{dd/MM/yyyy-HH:mm:ss} [%t] %M- %msg%n" />
		</Console>
		
	
		
	</Appenders>

	<Loggers>
		<Root level="all">
			<AppenderRef ref="console" level="all" />
					
		</Root>
	</Loggers>
</Configuration>